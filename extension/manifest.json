{
  "manifest_version": 2,
  "name": "Pictures New Tab",
  "description" : "Display interesting photos from Flickr on the New Tab page along with bookmarks from the Bookmarks Bar.",
  "version": "1.1.2",
  "homepage_url": "https://bitbucket.org/qiushihe",
  "icons": {
    "128": "icon_128.png",
    "48": "icon_48.png",
    "16": "icon_16.png"
  },
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "bookmarks",
    "storage",
    "unlimitedStorage"
  ]
}
